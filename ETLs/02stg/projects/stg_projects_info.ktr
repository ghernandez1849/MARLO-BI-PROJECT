    <connection>ORI_DB</connection>
    <sql>select pin.id, pin.title, pin.project_id, pin.id_phase, pin.start_date, pin.end_date, pin.type, pin.status, concat("P",pin.project_id) pid_name, concat("P",pin.project_id, ' - ',pin.title) composed_title,
-- concat("https://marlo.cgiar.org/projects/",trim(gu.acronym),"/description.do?projectID=",pin.project_id,"&amp;edit=true&amp;phaseID=", pin.id_phase) project_link_no,
"https://marlo.cgiar.org/projects/" project_link1,
"/description.do?projectID=" project_link2,
"&amp;edit=true&amp;phaseID=" project_link3,
gu.acronym,
ph.global_unit_id
from projects p
join projects_info pin on p.id = pin.project_id and p.is_active and pin.is_active
join phases ph on pin.id_phase = ph.id and ph.name = '${V_PHASE_NAME}' and ph.year = ${V_PHASE_YEAR}
join global_units gu on gu.id = ph.global_unit_id and gu.global_unit_type_id in (1,3)</sql>
