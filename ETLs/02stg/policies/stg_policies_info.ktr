    <connection>ORI_DB</connection>
    <sql>select pol.id policy_id,
if(ifnull(GROUP_CONCAT(distinct poli.title), '')='','&lt;Title Not Provided>', GROUP_CONCAT(distinct poli.title)) policy_title,
if(ifnull(GROUP_CONCAT(distinct poli.description), '')='', '&lt;Description Non Mandatory>',GROUP_CONCAT(distinct poli.description)) policy_description,
poli.id_phase,
max(poli.id) id,
gu.id global_unit_id, 
ifnull(poli.rep_ind_policy_investment_type_id, -1) rep_ind_policy_investment_type_id ,
ifnull(poli.rep_ind_stage_process_id, -1) rep_ind_stage_process_id,
poli.year,
pol.project_id,
if(gu.id in (27,28,17)
	,poli.external_link
	,concat('https://marlo.cgiar.org/summaries/',gu.acronym,'/projectPolicySummary.do?policyID=',pol.id,'&amp;phaseID=',ph.id)
	) pdf_url
FROM project_policies pol
join project_policy_info poli on pol.id=poli.project_policy_id
join phases ph on poli.id_phase=ph.id
JOIN global_units gu ON gu.id = ph.global_unit_id AND gu.is_active
join bi_phase_by_cubes pbc on pbc.cube_name = 'policies'
where ph.year = pbc.phase_year -- poli.`year`
and ph.name = pbc.phase_name -- 'AR'
-- and ph.`year` between ${START_YEAR} and ${END_YEAR}
and pol.is_active
-- and is_policy_excluded(poli.project_policy_id,poli.id_phase)=0
group by pol.id,ph.id,
gu.id , 
poli.rep_ind_policy_investment_type_id,
poli.rep_ind_stage_process_id, 
poli.year,
pol.project_id
;</sql>
