    <sql>select 
dext.deliverable_id	,
dext.phase_id	,
dext.source	,
dext.url	,
dext.doi	,
dext.title	,
dext.publication_type	,
dext.publication_year	,
dext.is_open_access	,
dext.open_access_link	,
dext.is_isi	,
dext.journal_name	,
dext.volume	,
dext.pages	,
dext.gardian_findability	,
dext.gardian_accessibility	,
dext.gardian_interoperability	,
dext.gardian_reusability	,
dext.gardian_title
FROM  deliverables dev
inner join deliverables_info devi on devi.deliverable_id = dev.id and devi.is_active
inner join projects pr on pr.id = dev.project_id
inner join projects_info pin on pin.project_id = dev.project_id and pin.id_phase = dev.id_phase
inner join phases ph on ph.id = devi.id_phase
inner join bi_phase_by_cubes pbc on pbc.cube_name = 'deliverables'
inner join deliverable_metadata_external_sources dext on dext.deliverable_id = devi.deliverable_id and dext.phase_id = devi.id_phase and dext.is_active
WHERE dev.is_active
and  pr.is_active
and  pin.is_active
and  ph.name = pbc.phase_name --  'AR'
AND ph. YEAR = pbc.phase_year --2021

union all

select 
dext.deliverable_id	,
dext.phase_id	,
dext.source	,
dext.url	,
dext.doi	,
dext.title	,
dext.publication_type	,
dext.publication_year	,
dext.is_open_access	,
dext.open_access_link	,
dext.is_isi	,
dext.journal_name	,
dext.volume	,
dext.pages	,
dext.gardian_findability	,
dext.gardian_accessibility	,
dext.gardian_interoperability	,
dext.gardian_reusability	,
dext.gardian_title
FROM  deliverables dev
inner join deliverables_info devi on devi.deliverable_id = dev.id and devi.is_active
inner join phases ph on ph.id = devi.id_phase
inner join bi_phase_by_cubes pbc on pbc.cube_name = 'deliverables'
inner join deliverable_metadata_external_sources dext on dext.deliverable_id = devi.deliverable_id and dext.phase_id = devi.id_phase and dext.is_active
where dev.is_active
and dev.project_id is null
and ph.year = ifnull(devi.new_expected_year, devi.year)
and length(trim(devi.title)) > 0
union all
select -1 deliverable_id	
,-1 phase_id	
,'Not provided' source	
,'Not provided' url	
,'Not provided' doi	
,'Not provided' title	
,'Not provided' publication_type	
,-1 publication_year	
,'Not provided' is_open_access	
,'Not provided' open_access_link	
,'Not provided' is_isi	
,'Not provided' journal_name	
,'Not provided' volume	
,'Not provided' pages	
,'Not provided' gardian_findability	
,'Not provided' gardian_accessibility	
,'Not provided' gardian_interoperability	
,'Not provided' gardian_reusability	
,'Not provided' gardian_title
;</sql>
