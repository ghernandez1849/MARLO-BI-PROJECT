    <sql>
-- Deliverables with project id not null have replication, so, those deliverables will be selected from the AR2021
SELECT 	
devi.id_phase,
ph.global_unit_id,
devi.deliverable_id,
dev.project_id,
if(ifnull(dcap.has_participants,0)=0,'NO','Yes') has_participants,
if(dcap.has_participants=1,ifnull(dcap.event_activity_name,'Not provided'),'Not applicable') activity_name, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
if(dcap.has_participants=1,ifnull(acty.`name`,'Not provided'),'Not applicable')  activity_type, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
if(dcap.has_participants=1,dcap.participants,0) participants,-- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
if(dcap.has_participants=1 and ifnull(dont_know_female,0)=0,dcap.females,0) females, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable AND if dont_know_female =1, 0 
if(dcap.has_participants=1,ifnull(part.`name`,'Not provided'),'Not applicable')  participant_type, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
ifnull(get_deliverable_metadata (devi.deliverable_id,devi.id_phase,'dc.language'),'Not mandatory') as 'language', -- null -> not mandatory
ifnull(get_deliverable_metadata (devi.deliverable_id,devi.id_phase,'cg:coverage.country'),'Not mandatory') as 'country',-- null -> not mandatory
ifnull(get_deliverable_metadata (devi.deliverable_id,devi.id_phase,'marlo.keywords'),'Not mandatory') as 'keywords',-- null -> not mandatory
if(devi.type_id in(64,66,80,63),if(ifnull(pm.publication_acknowledge,0)=1,'Yes','No'),'Not applicable') publication_acknowledge,-- if Categoria articles and books ->  publication_acknowledge else not applicable
ifnull(is_deliverable_findable(devi.deliverable_id,devi.id_phase),'Not applicable') 'findable',
ifnull(is_deliverable_accesible(devi.deliverable_id,devi.id_phase),'Not applicable') 'accesible',
ifnull(is_deliverable_interoperable(devi.deliverable_id,devi.id_phase),'Not applicable') 'interoperable',
ifnull(is_deliverable_reusable(devi.deliverable_id,devi.id_phase),'Not applicable') 'reusable'

FROM  deliverables dev
inner join deliverables_info devi on devi.deliverable_id = dev.id and devi.is_active
inner join phases ph on ph.id = devi.id_phase
inner join bi_phase_by_cubes pbc on pbc.cube_name = 'deliverables'
inner join projects pr on pr.id = dev.project_id
inner join projects_info pin on pin.project_id = dev.project_id and pin.id_phase = dev.id_phase
left join deliverable_participants dcap on dcap.deliverable_id = devi.deliverable_id and dcap.phase_id = devi.id_phase
left join rep_ind_type_activities acty on acty.id = dcap.rep_ind_type_activity_id
left join rep_ind_type_participants part on part.id = dcap.rep_ind_type_participant_id
left join deliverable_publications_metada pm on pm.deliverable_id=devi.deliverable_id and pm.id_phase = devi.id_phase
-- new tables
WHERE dev.is_active
and  pr.is_active
and  pin.is_active
and  ph.name =  pbc.phase_name -- 'AR'
AND ph. YEAR =  pbc.phase_year -- 2021
-- and dev.id=26548
-- AND ph.year between ${START_YEAR} and ${END_YEAR}  
union all

-- Deliverables with project id not null have replication, so, those deliverables will be selected from the AR2021

SELECT 	
devi.id_phase,
ph.global_unit_id,
devi.deliverable_id,
dev.project_id,
if(ifnull(dcap.has_participants,0)=0,'NO','Yes') has_participants,
if(dcap.has_participants=1,ifnull(dcap.event_activity_name,'Not provided'),'Not applicable') activity_name, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
if(dcap.has_participants=1,ifnull(acty.`name`,'Not provided'),'Not applicable')  activity_type, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
if(dcap.has_participants=1,dcap.participants,0) participants,-- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
if(dcap.has_participants=1 and ifnull(dont_know_female,0)=0,dcap.females,0) females, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable AND if dont_know_female =1, 0 
if(dcap.has_participants=1,ifnull(part.`name`,'Not provided'),'Not applicable')  participant_type, -- has_participants=yes y vacio -> not provided has_participants =no -> not applicable
ifnull(get_deliverable_metadata (devi.deliverable_id,devi.id_phase,'dc.language'),'Not mandatory') as 'language', -- null -> not mandatory
ifnull(get_deliverable_metadata (devi.deliverable_id,devi.id_phase,'cg:coverage.country'),'Not mandatory') as 'country',-- null -> not mandatory
ifnull(get_deliverable_metadata (devi.deliverable_id,devi.id_phase,'marlo.keywords'),'Not mandatory') as 'keywords',-- null -> not mandatory
if(devi.type_id in(64,66,80,63),if(ifnull(pm.publication_acknowledge,0)=1,'Yes','No'),'Not applicable') publication_acknowledge,-- if Categoria articles and books ->  publication_acknowledge else not applicable
ifnull(is_deliverable_findable(devi.deliverable_id,devi.id_phase),'Not applicable') 'findable',
ifnull(is_deliverable_accesible(devi.deliverable_id,devi.id_phase),'Not applicable') 'accesible',
ifnull(is_deliverable_interoperable(devi.deliverable_id,devi.id_phase),'Not applicable') 'interoperable',
ifnull(is_deliverable_reusable(devi.deliverable_id,devi.id_phase),'Not applicable') 'reusable'

FROM  deliverables dev
inner join deliverables_info devi on devi.deliverable_id = dev.id and devi.is_active
inner join phases ph on ph.id = devi.id_phase
-- inner join bi_phase_by_cubes pbc on pbc.cube_name = 'deliverables'
-- inner join projects pr on pr.id = dev.project_id
-- inner join projects_info pin on pin.project_id = dev.project_id and pin.id_phase = dev.id_phase
left join deliverable_participants dcap on dcap.deliverable_id = devi.deliverable_id and dcap.phase_id = devi.id_phase
left join rep_ind_type_activities acty on acty.id = dcap.rep_ind_type_activity_id
left join rep_ind_type_participants part on part.id = dcap.rep_ind_type_participant_id
left join deliverable_publications_metada pm on pm.deliverable_id=devi.deliverable_id and pm.id_phase = devi.id_phase
-- new tables
WHERE dev.is_active
and dev.project_id is null 
-- and  ph.name = pbc.phase_name --  'AR'
-- AND ph. YEAR = pbc.phase_year -- between 2018 and 2019
and ph.year = ifnull(devi.new_expected_year, devi.year)
-- and devi.title is not null
and length(trim(devi.title)) > 0
and ph.global_unit_id =1
-- and dev.id=26548
-- AND ph.year between ${START_YEAR} and ${END_YEAR}  
union all


SELECT 	
-1 id_phase,	
-1 global_unit_id,
-1 deliverable_id	,
null project_id	,
null has_participants	,
'Not applicable' activity_name	,
'Not applicable' activity_type	,
-1 participants	,
-1 females	,
'Not applicable' participant_type	,
'Not mandatory' language	,
'Not mandatory' country	,
'Not mandatory' keywords	,
'Not mandatory' publication_acknowledge	,
'Not applicable' findable	,
'Not applicable' accesible	,
'Not applicable' interoperable	,
'Not applicable' reusable
from dual
;


</sql>
