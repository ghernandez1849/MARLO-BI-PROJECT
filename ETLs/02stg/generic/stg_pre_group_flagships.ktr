    <connection>ORI_DB</connection>
    <sql>select pesi.id_phase, pes.id ent_id, 'oicr' ent_type,
ifnull(	group_concat(cpfl.name 
	 	 		 order by cpfl.acronym asc separator ', '), 'Not Provided') flagship_name_group, 
ifnull(group_concat(ifnull(concat(nullif(trim(cpfl.acronym),""), " - "), "") , cpfl.name 
				order by cpfl.acronym asc separator ', '), 'NP - Not Provided') flagship_acr_name_group,
ifnull(group_concat(nullif(trim(cpfl.acronym),"")
				order by cpfl.acronym asc separator ', '), 'NP') acronym_group, 
ifnull(group_concat(cpfl.id 
				order by cpfl.acronym asc separator ', '), -1) ids_group
from project_expected_studies pes 
INNER JOIN project_expected_study_info pesi ON pesi.project_expected_study_id = pes.id 
inner join phases ph on ph.id = pesi.id_phase
INNER JOIN bi_phase_by_cubes pbc on pbc.cube_name = 'oicrs'
left JOIN project_expected_study_flagships pesf ON pesf.expected_id = pes.id AND pesf.id_phase = ph.id
-- program type 1 = flagships global_unit_id = project expected study global unit id 
left JOIN crp_programs cpfl ON cpfl.id = pesf.crp_program_id AND cpfl.is_active AND cpfl.program_type = 1 and cpfl.global_unit_id = ph.global_unit_id
-- LEFT join projects_info pinf on pinf.project_id = pes.project_id and pinf.id_phase = pesi.id_phase
where  ph. NAME = pbc.phase_name
and ph.year = pbc.phase_year
and pes.is_active
and pesi.study_type_id = 1 -- OICR
and pesi.title is not null
group by pesi.id_phase, pes.id
;
</sql>
