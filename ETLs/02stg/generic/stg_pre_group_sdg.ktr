    <connection>ORI_DB</connection>
    <sql>-- oicrs y melias
select ssdg.id_phase, ssdg.ent_type, ssdg.ent_id,
ifnull(
group_concat(distinct ssdg.sdg_short_name
	 	 		order by ssdg.sdg_id asc separator '; '), 'Not Applicable') sdg_short_name_group,
ifnull(group_concat(distinct ssdg.sdg_target
	 	 		order by ssdg.sdg_id asc, ssdg.sdg_target_code asc separator '; '), 'Not Applicable') sdg_target_group,
ifnull(group_concat(distinct ssdg.sdg_indicator
	 	 		order by ssdg.sdg_id asc, ssdg.sdg_target_code asc, ssdg.sdg_indicator_codes separator '; '), 'Not Applicable') sdg_indicators_group,
ifnull(group_concat(distinct concat(ssdg.sdg_id, ';', ssdg.sdg_target_id, ';', ssdg.sdg_indicator_id)
	 	 		order by ssdg.sdg_id asc, ssdg.sdg_target_id asc, ssdg.sdg_indicator_id separator ', '), '-2;-2;-2') ids_group                
from 
(select ph.id id_phase, if(pesi.study_type_id = 1,'oicr', 'melia') ent_type, pes.id ent_id, dg.sub_ido_id sub_ido_id, dg.sdg_id sdg_id, 
dg.sdg_target sdg_target, 
dg.sdg_target_id sdg_target_id,
dg.sdg_target_code sdg_target_code, dg.sdg_short_name sdg_short_name, 
dg.sdg_description sdg_description, dg.sdg_indicator sdg_indicator, 
dg.sdg_indicator_codes, dg.sdg_indicator_id sdg_indicator_id
FROM phases ph
JOIN global_units gu ON gu.id = ph.global_unit_id AND gu.is_active
JOIN project_expected_studies pes ON pes.is_active
JOIN project_expected_study_info pesi ON pesi.project_expected_study_id = pes.id AND pesi.id_phase = ph.id
JOIN bi_phase_by_cubes pbc on pbc.cube_name = 'oicrs'
left JOIN project_expected_study_sub_ido pess ON pess.expected_id = pes.id AND pess.id_phase = pesi.id_phase
left JOIN
( select sdg.id sdg_id, concat(sdgt.sdg_target_code, ' - ', sdgt.sdg_target) sdg_target, sdgt.id sdg_target_id, idoxsdg.sub_ido_id,
	sdgt.sdg_target_code, sdg.short_name sdg_short_name, sdg.description sdg_description, 
	concat(sdgi.sdg_indicator_codes, ' - ', sdgi.sdg_indicator) sdg_indicator, sdgi.sdg_indicator_codes, sdgi.id sdg_indicator_id
from	sub_ido_sdg_target idoxsdg 
 join sdg_targets sdgt on sdgt.id = idoxsdg.sdg_target_id
 JOIN sdg_indicators sdgi on sdgi.sdg_target_id = sdgt.id
 JOIN sustainable_development_goals sdg on sdg.id = sdgt.sdg_id
union all
select -1 sdg_id, 'Not Provided' sdg_target, -1 sdg_target_id, -1 sub_ido_id, 
'NP' sdg_target_code,  'Not Provided' sdg_short_name, 'Not Provided' sdg_description,
'Not Provided' sdg_indicator, 'NP' sdg_indicator_codes, -1 sdg_indicator_id 
from dual
) dg on dg.sub_ido_id =  ifnull(pess.sub_ido_id, -1)
where ph. NAME = pbc.phase_name
AND ph. YEAR = pbc.phase_year
-- AND ph.year between ${START_YEAR} and ${END_YEAR} 
and pes.is_active
-- and pesi.study_type_id = 1
and pesi.title is not null
) ssdg 
group by ssdg.id_phase, ssdg.ent_type, ssdg.ent_id;</sql>
