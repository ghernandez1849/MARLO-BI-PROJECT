    <connection>ORI_DB</connection>
    <sql>select mm.id_phase, mm.ent_id, mm.ent_type,
ifnull( group_concat(if(mm.is_primary, concat('{Primary} ', mm.milestone_title), mm.milestone_title)
	 	 		order by mm.is_primary desc, mm.milestone_composed_id asc separator '; '), 
	 if(mm.has_milestones_calc = 1, 'Not Provided', 'Not Applicable')) milestone_title_group, 
ifnull( group_concat(if(mm.is_primary, concat('{Primary} ', mm.outcome_description), mm.outcome_description)
	 	 		order by mm.is_primary desc, mm.outcome_composed_id asc separator '; ') , 
	if(mm.has_milestones_calc = 1, 'Not Provided', 'Not Applicable')) outcome_description_group ,
ifnull(group_concat(distinct concat(mm.id_phase, ";", mm.outcome_composed_id, ";",mm.milestone_composed_id ,";", mm.is_primary) 
	 	 		order by mm.is_primary desc, mm.id_phase  asc, mm.outcome_composed_id asc, mm.milestone_composed_id asc  separator ', '),
                 if(mm.has_milestones_calc = 1, '-1;-1;-1', '-2;-2;-2')) ids_group
from 
(
select ph.id id_phase, pin.id ent_id, 'innovation' ent_type, mil.composed_id milestone_composed_id, clean_html_tags(mil.title) milestone_title, 
outc.composed_id outcome_composed_id, clean_html_tags(outc.description) outcome_description,  ifnull(imil.is_primary,0) is_primary,
if(mil.composed_id is not null, ifnull(pii.has_milestones, 1), ifnull(pii.has_milestones,0)) has_milestones_calc
from project_innovations pin 
JOIN project_innovation_info pii ON pii.project_innovation_id = pin.id
join phases ph on ph.id = pii.id_phase
join bi_phase_by_cubes pbc on pbc.cube_name = 'innovations'
join global_units gu on gu.id = ph.global_unit_id
left JOIN project_innovation_milestones imil on imil.project_innovation_id = pii.project_innovation_id and imil.id_phase = pii.id_phase 
left join crp_milestones mil on mil.id = imil.crp_milestone_id and mil.is_active
left join crp_program_outcomes outc on outc.id = mil.crp_program_outcome_id and outc.is_active
where 
-- AND ph.year between ${START_YEAR} and ${END_YEAR} 
ph. YEAR = pbc.phase_year
AND ph. NAME = pbc.phase_name
and pin.is_active
) mm
group by mm.id_phase, mm.ent_id, mm.ent_type;
</sql>
