    <sql>select mm.id_phase, mm.ent_id, mm.ent_type,
group_concat(if(mm.is_primary, concat('{Primary} ', mm.milestone_title), mm.milestone_title)
	 	 		order by mm.is_primary desc, mm.milestone_composed_id asc separator '; ') milestone_title_group, 
group_concat(if(mm.is_primary, concat('{Primary} ', mm.outcome_description), mm.outcome_description)
	 	 		order by mm.is_primary desc, mm.outcome_composed_id asc separator '; ') outcome_description_group ,
group_concat(distinct concat(mm.id_phase, ";", mm.outcome_composed_id, ";",mm.milestone_composed_id ,";", mm.is_primary) 
	 	 		order by mm.is_primary desc, mm.id_phase  asc, mm.outcome_composed_id asc, mm.milestone_composed_id asc  separator ', ') ids_group
from 
(
select ph.id id_phase, pes.id ent_id, 'oicr' ent_type, mil.composed_id milestone_composed_id, clean_html_tags(mil.title) milestone_title, 
outc.composed_id outcome_composed_id, clean_html_tags(outc.description) outcome_description,  ifnull(omil.is_primary,0) is_primary
from project_expected_studies pes 
JOIN project_expected_study_info pesi ON pesi.project_expected_study_id = pes.id 
join phases ph on ph.id = pesi.id_phase
join global_units gu on gu.id = ph.global_unit_id
join project_expected_study_milestones omil on omil.expected_id = pesi.project_expected_study_id and omil.id_phase = pesi.id_phase
join crp_milestones mil on mil.id = omil.crp_milestone_id and mil.is_active
join crp_program_outcomes outc on outc.id = mil.crp_program_outcome_id and outc.is_active
join bi_phase_by_cubes pbc on pbc.cube_name = 'oicrs'
where 
-- AND ph.year between ${START_YEAR} and ${END_YEAR} 
ph. YEAR = pbc.phase_year
AND ph. NAME = pbc.phase_name
and pes.is_active
and pesi.study_type_id = 1
and pesi.title is not null
union all
select poli.id_phase, poli.project_policy_id ent_id, 'policy' ent_type, 
pmile.composed_id milestone_composed_id, 
clean_html_tags(pmile.title) milestone_title, 
poutc.composed_id outcome_composed_id, clean_html_tags(poutc.description) outcome_description,
ifnull(ppmile.is_primary,0) is_primary
from project_policies pol
join project_policy_info poli on pol.id=poli.project_policy_id
join phases ph on ph.id=poli.id_phase
join policy_milestones ppmile on ppmile.policy_id = poli.project_policy_id and ppmile.id_phase = poli.id_phase
join crp_milestones pmile on pmile.id = ppmile.crp_milestone_id
join crp_program_outcomes poutc on poutc.id = pmile.crp_program_outcome_id and poutc.is_active
join bi_phase_by_cubes pbc on pbc.cube_name = 'policies'
where ph.year = pbc.phase_year -- poli.`year`
-- and is_policy_excluded(poli.project_policy_id,poli.id_phase)=0
-- and ph.`year` between ${START_YEAR} and ${END_YEAR}
-- and ph.`year` BETWEEN 2019 AND 2019
and ph.name = pbc.phase_name -- 'AR'
and pol.is_active
) mm
group by mm.id_phase, mm.ent_id, mm.ent_type;</sql>
