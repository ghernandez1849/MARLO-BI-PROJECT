    <sql>SELECT gu.acronym CRP, concat('C',p.id) 'project id',
CONCAT(
		'https://aiccra.marlo.cgiar.org/clusters/',
		gu.acronym,
		'/description.do?edit=true&amp;projectID=',
		p.id,
		'&amp;phaseID=',
		ph.id
	)	 AS 'Project link',
	(
		CASE
		WHEN ISNULL(pi.title) THEN
			"&lt;Not Defined>"
		ELSE
			pi.title
		END
	) AS 'Project Title',
	if(pi.administrative=1,'Administrative',ct.name) 'Project type',
	(CASE
		WHEN pi.administrative = 1 THEN
			"&lt;Not Applicable>"
		ELSE
			if(fp.program_type = 1,fp.acronym,'')
		END
	) Flagships,
(CASE
		WHEN pi.administrative = 1 THEN
			"&lt;Not Applicable>"
		ELSE
			if(fp.program_type = 2,fp.acronym,'')
		END
	) Regions,
	(
		CASE pi. STATUS
		WHEN 2 THEN
			"On-going"
		WHEN 3 THEN
			"Complete"
		WHEN 4 THEN
			"Extended"
		WHEN 5 THEN
			"Cancelled"
		END
	) AS "Status",
	(
		CASE
		WHEN li.crp_program IS NOT NULL
		OR li.institution_id IS NULL THEN
			 li.`name`
		WHEN i.id IS NOT NULL THEN
			 i.`name`
		ELSE
			""
		END
	) AS "Management Liaison",
	(
		CASE
		WHEN li.crp_program IS NOT NULL
		OR li.institution_id IS NULL THEN
			li.acronym
		WHEN i.id IS NOT NULL THEN
			i.acronym
		ELSE
			"&lt;Not Defined>"
		END
	) AS "Management Liaison acronym",
	CONCAT(
		u.first_name,
		", ",
		u.last_name,
		"&lt;",
		u.email,
		">"
	) AS "Management Liaison Contact Person",
(
		CASE
		WHEN COUNT(i2.id)=0 THEN
			"&lt;Not Defined>"
		WHEN GROUP_CONCAT(DISTINCT  i2.acronym) IS NOT NULL AND GROUP_CONCAT(DISTINCT  i2.acronym)!="" THEN
			GROUP_CONCAT(DISTINCT  i2.acronym, "-", i2.`name`)
		ELSE
			GROUP_CONCAT(DISTINCT  i2.`name`)
		END
	) AS "Lead Institution",
 GROUP_CONCAT(DISTINCT
		u2.first_name,
		", ",
		u2.last_name,
		"&lt;",
		u2.email,
		">"
	) AS "Project Leader",
	(
		CASE
		WHEN s.id IS NULL THEN
			"Not Submitted"
		WHEN s.unsubmit = 0
		OR s.unsubmit IS NULL THEN
			"Submitted"
		ELSE
			"Returned"
		END
	) AS "Submission_status",
	(	CASE
		WHEN s.id IS NULL THEN
			"&lt;Not Defined>"
		WHEN s.unsubmit = 0
		OR s.unsubmit IS NULL THEN
			concat(subusr.first_name,' ',subusr.last_name)
		ELSE
			concat(unsubusr.first_name,' ',unsubusr.last_name)
 			-- "&lt;Not Defined>"
		END
  ) submitted_by, 
	(
		CASE
		WHEN s.id IS NULL THEN
			"&lt;Not Defined>"
		WHEN s.unsubmit = 0
		OR s.unsubmit IS NULL THEN
			CONCAT(
				"Submitted on ",
				s.date_time
			)
		ELSE
			CONCAT(
				"Unsubmitted on ",
				s.date_time
			)
		END
	) AS "Submission Date",
	(
		CASE
		WHEN ISNULL(
			s.modification_justification
		)
		OR s.modification_justification = "" THEN
			"&lt;Not Defined>"
		ELSE
			s.modification_justification
		END
	) AS "Submission Justification",
	(
		CASE
		WHEN ISNULL(s.unsubmit_justification)
		OR s.unsubmit_justification = "" THEN
			"&lt;Not Defined>"
		ELSE
			s.unsubmit_justification
		END
	) AS "Unsubmission Justification",
sec.id section_id
,ifnull(nsec.new_value,sec.section_name) section_name
-- ,sec.section_name
,CASE 
when 
group_concat(distinct sec.missing_fields) is not null and group_concat(distinct missing_fields)!="" 
then "Incomplete"
else 
"Complete" 
end AS "Section status"
,CASE 
when section_name ='deliverableList' then concat('D',di.deliverable_id)
when section_name ='studies' then concat('OI',pesi.project_expected_study_id)
when section_name ='innovationsList' then concat('IN',inni.project_innovation_id)
when section_name ='policies' then concat('PO',poli.project_policy_id)
when section_name ='contributionsCrpList' then concat('''',outc.composed_id)
ELSE 'ALL'
end as indicator_ID
,CASE 
when section_name ='deliverableList' then concat('D',sec.deliverable_id)
when section_name ='studies' then concat('OI',sec.project_expected_id)
when section_name ='innovationsList' then concat('IN',sec.project_innovation_id)
when section_name ='policies' then concat('PO',sec.project_policy_id)
when section_name ='contributionsCrpList' then concat('''',sec.project_outcome_id)
ELSE 'ALL'
end as indicator_ID_2
,group_concat(distinct sec.missing_fields) AS "missing fields"
,if(gu.acronym = 'CCAFS' and IFNULL(pi.is_evaluated,0)= 0 ,'No','Yes') is_evaluated
,sysdate() updated_date
,concat(ph.name,ph.year) Phase
,ph.name phase_name
,ph.year phase_year
FROM projects p
JOIN projects_info pi ON pi.project_id = p.id
JOIN bi_parameters bip on bip.parameter_name='project_status_phase' 
JOIN phases ph ON ph.id = pi.id_phase and concat(ph.name,ph.year)=bip.parameter_value
JOIN global_units gu ON gu.id = ph.global_unit_id and gu.is_active
LEFT JOIN cluster_types ct ON ct.id = pi.type_id

LEFT join project_focuses pfp on pfp.project_id = pi.project_id and pfp.id_phase = pi.id_phase and pfp.is_active
LEFT join crp_programs fp on fp.id = pfp.program_id and fp.is_active and fp.global_unit_id = gu.id
LEFT JOIN liaison_institutions li ON li.id = pi.liaison_institution_id AND li.is_active
LEFT JOIN liaison_users lu ON lu.id = pi.liaison_user_id AND lu.is_active
LEFT JOIN institutions i ON i.id = li.institution_id
LEFT JOIN users u ON u.id = lu.user_id
LEFT JOIN project_partners pp ON pp.project_id = p.id AND pp.is_active AND pp.id_phase =ph.id
LEFT JOIN project_partner_persons ppp ON pp.id = ppp.project_partner_id AND ppp.is_active AND ppp.contact_type="PL"
LEFT JOIN project_partners pp2 ON pp2.id = ppp.project_partner_id and pp2.is_active
LEFT JOIN institutions i2 ON i2.id = pp2.institution_id
LEFT JOIN users u2 ON u2.id = ppp.user_id AND u2.is_active
LEFT JOIN submissions s ON s.id = (
	SELECT
		MAX(sq.id) sq
	FROM
		submissions sq
	WHERE
		sq.project_id = p.id
	AND sq.cycle = ph.description
	AND sq.`year` = ph.`year`
)
left join users subusr on subusr.id = s.user_id
left join users unsubusr on unsubusr.id = s.unsubmit_user_id
LEFT JOIN section_statuses sec  ON sec.project_id=p.id  AND sec.cycle = ph.description AND sec.year=ph.year 
LEFT JOIN bi_homologation_fields nsec on nsec.Table_value = sec.section_name and nsec.`Table`='section_statuses' and nsec.Field='section_name' and scope = 'Project'

-- only validate year = phase_year
left join deliverables_info di on di.deliverable_id = sec.deliverable_id and di.id_phase = ph.id  and di.is_active and ifnull(if(di.new_expected_year=-1,null,di.new_expected_year), di.`year`) = ph.year

left join project_expected_studies pes on pes.id= sec.project_expected_id and pes.is_active
left join project_expected_study_info pesi on pesi.project_expected_study_id= pes.id and pesi.id_phase = ph.id and pesi.year >= ph.year

left join project_innovations inn on inn.id= sec.project_innovation_id and inn.is_active
left join project_innovation_info inni on inni.project_innovation_id =inn.id and inni.id_phase = ph.id and inni.`year` >= ph.`year`
-- only validate year = phase_year
left join project_policies pol on pol.id= sec.project_policy_id and pol.is_active
left join project_policy_info poli on poli.project_policy_id = pol.id and poli.id_phase = ph.id and poli.`year` = ph.year 

left join project_outcomes pout on pout.id = sec.project_outcome_id and pout.is_active
left join crp_program_outcomes outc on pout.outcome_id = outc.id and outc.is_active
 WHERE p.is_active
-- AND ph.year =2022 
-- and ph.name ='APWB'
-- and p.id = 372
and gu.acronym = 'AICCRA'
-- AND ph.year = 2021
-- and ph.name ='AR'
and ph.year between year(pi.start_date) and year(pi.end_date)
GROUP BY
	p.id,pi.title,pi.administrative,pi.status,pi.is_evaluated,
  li.crp_program,li.institution_id,li.name,li.acronym,
	u.first_name,u.last_name,u.email,
	s.id,
  fp.program_type,fp.acronym,
  ph.id,
  sec.id,sec.section_name,sec.deliverable_id,sec.project_expected_id,sec.project_innovation_id,sec.project_policy_id,sec.project_outcome_id
;

