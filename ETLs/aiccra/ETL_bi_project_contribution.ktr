  <step>
    <name>Load_ project_contribution</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ORI_AICCRA</connection>
    <sql>SELECT  DISTINCT
	ph.id "ID Phase", ph.`name` "Phase name", ph.`year` "Phase year",
	gu.acronym CRP,gu.name "CRP name",
	concat('C',p.id) "Project ID",
	CONCAT(
		'https://aiccra.marlo.cgiar.org/projects/',
		gu.acronym,
		'/contributionCrp.do?projectOutcomeID=',
		pout.id,
		'&amp;edit=true&amp;phaseID=',
		get_max_open_phase(gu.id)
	) AS 'Project Link'
	,pi.title "Project Title"
	,ct.`name` "Project type"
	, fp.acronym "Flagship" 
	,outc.composed_id "Outcome ID"
	, if(ph.`name`='POWB',pout.expected_value,0) "Outcome expected value"
	, if(ph.`name`='POWB',exunit.`name`,'') "Outcome expected unit"
	-- , if(ph.`name`='AR',pout.expected_value,0) "Outcome reported value"
	-- , if(ph.`name`='AR',exunit.`name`,'') "Outcome reported unit"
	, mil.composed_id "Milestone ID"
	, mil.title "Milestone"
	-- , mil.year
	, pmil.expected_value "Milestone expected value"
	, munit.`name` "Milestone expected unit"
	, pmil.progress_value "Milestone progress value"
	, pmil.achieved_value "Milestone reported value"
	, pmil.setted_value "Milestone setted value"
	-- , if(ph.`name`='AR',munit.`name`,'') "Milestone reported unit"
	, pout.narrative_target "Contribution Narrative"
	, com.communication "Outcome Comunication" 
	, if(mil.id is null,'&lt;Not provided>',pmil.narrative_target) "Milestone expected narrative"
	, if(mil.id is null,'&lt;Not provided>',pmil.narrative_progress) "Milestone progress narrative"
	, if(mil.id is null,'&lt;Not provided>',pmil.narrative_achieved) "Milestone achieved narrative"
	, if(next.id is null,'&lt;Not provided>',next.next_user) "Next user"
	, if(next.id is null,'&lt;Not provided>',next.knowledge) "Next user knowledge expected"
	, if(next.id is null,'&lt;Not provided>',next.strategies) "Next user Strategies"
	, if(next.id is null,'&lt;Not provided>',next.knowledge_report) "Next user knowledge progress"
	, if(next.id is null,'&lt;Not provided>',next.strategies_report) "Next user Strategies Progress"
	, outi2.indicator as 'Progress question'
	, (select ifnull(GROUP_CONCAT(pouti2.narrative),'') from project_outcome_indicators pouti2 where pouti2.project_outcome_id = pout2.id and pouti2.crp_outcome_indicator=outi2.id and pouti2.is_active) 'Progress Narrative'
	, outi.indicator 'Report question'
	, (select ifnull(GROUP_CONCAT(pouti.narrative),'') from project_outcome_indicators pouti where pouti.project_outcome_id = pout.id and pouti.crp_outcome_indicator=outi.id and pouti.is_active) 'Report Narrative'
	, concat(ph.id,'_',mil.composed_id) pk
	, concat(p.id,'_',outc.composed_id,'_',mil.year) contribution_pk
	, pcl.lessons 	
	FROM	projects p
	INNER JOIN projects_info pi ON pi.project_id = p.id
	JOIN cluster_types ct on ct.id = pi.type_id
	-- join bi_parameters bpa on bpa.parameter_name='BI_data_phase'
	INNER JOIN phases ph ON ph.id = pi.id_phase -- and ph.id = bpa.parameter_value
	INNER JOIN global_units gu ON gu.id = ph.global_unit_id

	JOIN project_outcomes pout on pout.project_id = pi.project_id and pout.id_phase = pi.id_phase  and pout.is_active
	
	JOIN crp_program_outcomes outc on outc.id = pout.outcome_id
	JOIN crp_programs fp on fp.id = outc.crp_program_id
	JOIN srf_target_units unit on unit.id = outc.target_unit_id
	left join crp_program_outcome_indicator outi on outi.crp_program_outcome_id = outc.id and outi.is_active
	
	
	-- Questions from progress
	left join phases ph2 on ph2.year=ph.year and ph2.global_unit_id = ph.global_unit_id and ph2.name='Progress'
	left join crp_program_outcomes outc2 on outc.composed_id = outc2.composed_id and outc2.id_phase = ph2.id
  left JOIN project_outcomes pout2 on pout2.project_id = pi.project_id and pout2.id_phase = ph2.id and outc2.id = pout2.outcome_id and pout2.is_active
  left join crp_program_outcome_indicator outi2 on outc2.id = outi2.crp_program_outcome_id and outi2.is_active	

	LEFT JOIN srf_target_units exunit on exunit.id = pout.expected_unit
	LEFT JOIN project_communications com on com.project_outcome_id =pout.id and com.is_active
	LEFT JOIN project_nextusers next on next.project_outcome_id =pout.id and next.is_active

	left join project_milestones pmil on pmil.project_outcome_id = pout.id and pmil.is_active
	LEFT JOIN crp_milestones mil on mil.id = pmil.crp_milestone_id
	LEFT JOIN srf_target_units munit on munit.id = pmil.expected_unit
	left join project_component_lessons pcl on pcl.component_name='contributionCrp' and pcl.id_phase = pout.id_phase and pcl.project_outcome_id = pout.id
	WHERE	p.is_active
	-- and  pi.project_id = 102084
  -- and  mil.title='IPI 1.3: Satisfaction with the quality and usefulness of climate?relevant knowledge products, decision-making tools and services received under AICCRA expressed by surveyed partners and stakeholders express (%)'
	and ph.name = 'AR'
	-- and ph.year = 2022
	and ph.year = mil.year
	and gu.acronym = 'AICCRA'
	;</sql>
